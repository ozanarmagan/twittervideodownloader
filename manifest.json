{
    "name":"Twitter Video Downloader",
    "description":"Download Twitter videos easily",
    "version":"0.1",
    "manifest_version":2,
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":[
                "content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "defult_title": "Download"
    },
   "permissions":["tabs"]
}